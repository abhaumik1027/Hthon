<% if(message == 'search') { %> 
    <div class="alert alert-info" role="alert">The A-Link you are looking for does not exists. You are welcome to create one.</div>
        
<% } %>
<% if(message == 'url_exists') { %> 
    <div class="alert alert-warning" role="alert">The URL you want to add already exists.</div>
        
<% } %>
<% if(message == 'alink_exists') { %> 
    <div class="alert alert-warning" role="alert">The A-Link you want to add already exists.</div>
        
<% } %>
<% if(message == 'invalid_url') { %> 
    <div class="alert alert-warning" role="alert">Please enter a valid url.</div>
        
<% } %>
<% if(message == 'alink_added') { %> 
    <div class="alert alert-success" role="alert">Alink is successfully added.</div>
        
<% } %>
<% if(message == 'alink_updated') { %> 
    <div class="alert alert-success" role="alert">Alink is successfully updated.</div>
        
<% } %>
<% if(message == 'alink_deleted') { %> 
    <div class="alert alert-success" role="alert">Alink is successfully deleted.</div>
        
<% } %>